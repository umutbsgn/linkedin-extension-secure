{
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@vercel/node" },
        { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/anthropic/analyze", "dest": "/api/anthropic/analyze.js" },
        { "src": "/api/(.*)", "dest": "/index.js" },
        { "src": "/(.*)", "dest": "/index.js" }
    ]
}