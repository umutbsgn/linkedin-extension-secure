{
    "version": 2,
    "rewrites": [
        { "source": "/api/posthog/track", "destination": "/api/posthog/track" },
        { "source": "/api/anthropic/analyze", "destination": "/api/anthropic/analyze" },
        { "source": "/api/supabase/proxy", "destination": "/api/supabase/proxy" },
        { "source": "/api/supabase/beta-access", "destination": "/api/supabase/beta-access" },
        { "source": "/api/supabase/user-settings", "destination": "/api/supabase/user-settings" },
        { "source": "/api/supabase/auth/login", "destination": "/api/supabase/auth/login" },
        { "source": "/api/supabase/auth/signup", "destination": "/api/supabase/auth/signup" }
    ],
    "env": {
        "ANTHROPIC_API_KEY": "@anthropic_api_key",
        "POSTHOG_API_KEY": "@posthog_api_key",
        "POSTHOG_API_HOST": "@posthog_api_host",
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_SERVICE_KEY": "@supabase_service_key",
        "SUPABASE_ANON_KEY": "@supabase_anon_key"
    }
}